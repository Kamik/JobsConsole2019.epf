			строкаПараметра.ЗначениеПараметра = ПолучитьИзВременногоХранилища(значПараметра);	
		КонецЕсли;
	КонецЦикла;
	рез = Новый Структура;
	рез.Вставить(""Текст"",текДанные.ТекстЗапроса);
	рез.Вставить(""Параметры"",тзПараметры);	
	Возврат ЗначениеВСтрокуВнутр(рез);
