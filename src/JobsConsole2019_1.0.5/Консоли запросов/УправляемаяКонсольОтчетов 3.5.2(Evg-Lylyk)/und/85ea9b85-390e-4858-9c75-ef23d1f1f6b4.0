		КонецЦикла;
		
	ИначеЕсли ТипЗнч(Объект) = Тип (""Структура"") Тогда
		
		Для Каждого Элемент Из Объект Цикл
			КопияОбъекта.Вставить(Элемент.Ключ, Элемент.Значение);
		КонецЦикла;
		
	КонецЕсли;
	
	Возврат КопияОбъекта;
	
